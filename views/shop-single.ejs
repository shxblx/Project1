<script>
  $(document).ready(function () {
    // Initialize Owl Carousel
    $(".item-entry").owlCarousel({
      items: 1,
      loop: true,
      autoplay: true,
      autoplayHoverPause: true,
      nav: true,
      dots: false
    });

    // Initialize magnify.js for zoom effect
